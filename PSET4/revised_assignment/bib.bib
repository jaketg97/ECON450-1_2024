}

@article{goolsbee_consumer_2004,
	title = {The {Consumer} {Gains} from {Direct} {Broadcast} {Satellites} and the {Competition} with {Cable} {TV}},
	volume = {72},
	issn = {1468-0262},
	url = {https://onlinelibrary.wiley.com/doi/abs/10.1111/j.1468-0262.2004.00494.x},
	doi = {10.1111/j.1468-0262.2004.00494.x},
	abstract = {This paper examines direct broadcast satellites (DBS) as a competitor to cable. We first estimate a structural consumer level demand system for satellite, basic cable, premium cable and local antenna using micro data on almost 30000 households in 317 markets, including extensive controls for unobserved product quality and allowing the distribution of unobserved tastes to follow a fully flexible multivariate normal distribution. The estimated elasticity of expanded basic is about -15, with the demand for premium cable and DBS more elastic. The results identify strong correlations in the taste for different products not captured in conventional logit models. Estimates of the supply response of cable suggest that without DBS entry cable prices would be about 15 percent higher and cable quality would fall. We find a welfare gain of between {\textbackslash}127 and {\textbackslash}190 per year (aggregate {\textbackslash}2.5 billion) for satellite buyers, and about {\textbackslash}50 (aggregate \$3 billion) for cable subscribers.},
	language = {en},
	number = {2},
	urldate = {2023-11-07},
	journal = {Econometrica},
	author = {Goolsbee, Austan and Petrin, Amil},
	year = {2004},
	note = {\_eprint: https://onlinelibrary.wiley.com/doi/pdf/10.1111/j.1468-0262.2004.00494.x},
	keywords = {cable competition, multinomial probit, satellite dish, Television},
	pages = {351--381},
	file = {Goolsbee and Petrin - 2004 - The Consumer Gains from Direct Broadcast Satellite.pdf:/Users/jacobgosselin/Zotero/storage/GCM7GASI/Goolsbee and Petrin - 2004 - The Consumer Gains from Direct Broadcast Satellite.pdf:application/pdf;Snapshot:/Users/jacobgosselin/Zotero/storage/PNC978U4/j.1468-0262.2004.00494.html:text/html},
}

@article{hastings_sales_2017,
	title = {Sales {Force} and {Competition} in {Financial} {Product} {Markets}: {The} {Case} of {Mexico}'s {Social} {Security} {Privatization}},
	volume = {85},
	copyright = {© 2017 The Econometric Society},
	issn = {1468-0262},
	shorttitle = {Sales {Force} and {Competition} in {Financial} {Product} {Markets}},
	url = {https://onlinelibrary.wiley.com/doi/abs/10.3982/ECTA12302},
	doi = {10.3982/ECTA12302},
	abstract = {This paper examines how sales force impacts competition and equilibrium prices in the context of a privatized pension market. We use detailed administrative data on fund manager choices and worker characteristics at the inception of Mexico's privatized social security system, where fund managers had to set prices (management fees) at the national level, but could select sales force levels by local geographic areas. We develop and estimate a model of fund manager choice where sales force can increase or decrease customer price sensitivity. We find exposure to sales force lowered price sensitivity, leading to inelastic demand and high equilibrium fees. We simulate oft proposed policy solutions: a supply-side policy with a competitive government player and a demand-side policy that increases price elasticity. We find that demand-side policies are necessary to foster competition in social safety net markets with large segments of inelastic consumers.},
	language = {en},
	number = {6},
	urldate = {2023-11-07},
	journal = {Econometrica},
	author = {Hastings, Justine and Hortaçsu, Ali and Syverson, Chad},
	year = {2017},
	note = {\_eprint: https://onlinelibrary.wiley.com/doi/pdf/10.3982/ECTA12302},
	keywords = {competition in the financial sector, marketing, Retirement investments},
	pages = {1723--1761},
	file = {Full Text PDF:/Users/jacobgosselin/Zotero/storage/Q29PRJ29/Hastings et al. - 2017 - Sales Force and Competition in Financial Product M.pdf:application/pdf;Snapshot:/Users/jacobgosselin/Zotero/storage/P5J5GK3K/ECTA12302.html:text/html},
}

