@article{conlon_best_2020,
	title = {Best practices for differentiated products demand estimation with {PyBLP}},
	volume = {51},
	issn = {0741-6261, 1756-2171},
	url = {https://onlinelibrary.wiley.com/doi/10.1111/1756-2171.12352},
	doi = {10.1111/1756-2171.12352},
	abstract = {Diﬀerentiated products demand systems are a workhorse for understanding the price eﬀects of mergers, the value of new goods, and the contribution of products to seller networks. Berry, Levinsohn, and Pakes (1995) provide a ﬂexible random coeﬃcients logit model which accounts for the endogeneity of prices. This article reviews and combines several recent advances related to the estimation of BLP-type problems and implements an extensible generic interface via the PyBLP package. Monte Carlo experiments and replications suggest diﬀerent conclusions than the prior literature: multiple local optima appear to be rare in well-identiﬁed problems; good performance is possible even in small samples, particularly when “optimal instruments” are employed along with supply-side restrictions.},
	language = {en},
	number = {4},
	urldate = {2023-11-07},
	journal = {The RAND Journal of Economics},
	author = {Conlon, Christopher and Gortmaker, Jeff},
	month = dec,
	year = {2020},
	pages = {1108--1161},
	file = {Conlon and Gortmaker - 2020 - Best practices for differentiated products demand .pdf:/Users/jacobgosselin/Zotero/storage/62ZABMPC/Conlon and Gortmaker - 2020 - Best practices for differentiated products demand .pdf:application/pdf},
}

